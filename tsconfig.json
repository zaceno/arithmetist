{
  "compilerOptions": {
    "checkJs": true,
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitThis": false,
    "target": "esnext",
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "hyperapp": [".types/hyperapp.d.ts"],
      "@hyperapp/html": [".types/hyperapp-html.d.ts"],
      "@hyperapp/svg": [".types/hyperapp-svg.d.ts"]
    }
  },
  "include": [".types/*.d.ts", "**/*.js", "**/*.d.ts"]
}
